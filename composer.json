{
    "name": "aw-studio/laravel-sites",
    "license": "MIT",
    "authors": [
        {
            "name": "jannescb",
            "email": "jannes@aw-studio.de"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "autoload": {
        "psr-4": {
            "AwStudio\\Sites\\": "src"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8",
        "astrotomic/laravel-fileable": "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.19",
        "mockery/mockery": "^1.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AwStudio\\Sites\\SitesServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
